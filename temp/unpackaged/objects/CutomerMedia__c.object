        <label>CutomerMedia Name</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>StudentMedias</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Available_Media_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>Media__r.Total_Media__c  -  Media__r.Media_Issued__c  = 0</errorConditionFormula>
        <errorDisplayField>Media__c</errorDisplayField>
        <errorMessage>Media Not Available Try another</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Book_Issued</fullName>
        <active>false</active>
        <errorConditionFormula>OR( Dues_Penalties__c  &gt; 0, Customer__r.Total_Penalties__c  &gt; 0)</errorConditionFormula>
        <errorMessage>The customer is blacklisted!!!
First, clear all its dues</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EMail_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>NOT( Customer__r.Email_id__c  =  Email_id__c )</errorConditionFormula>
        <errorMessage>Email entered in customer  should be equal</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
